{
  "name": "smart-web-demo",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "jest --verbose",
    "start": "./node_modules/.bin/webpack-dev-server --port 3000",
    "clean": "rm -rf ./dist",
    "build": "NODE_ENV=production ./node_modules/.bin/webpack --config webpack.config.babel.js --progress --color",
    "build:dll": "NODE_ENV=production ./node_modules/.bin/webpack --config webpack.dll.babel.js --progress --color",
    "build:pro": "NODE_ENV=production ./node_modules/.bin/webpack --config webpack.pro.babel.js --progress --color",
    "build:all": "NODE_ENV=production npm run clean && npm run build:dll && npm run build:pro"
  },
  "devDependencies": {
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-env": "1.1.4",
    "babel-preset-react": "6.16.0",
    "html-webpack-plugin": "2.24.1",
    "webpack": "2.2.0-rc.3",
    "webpack-dev-server": "1.16.2"
  },
  "dependencies": {
    "iflux2": "1.2.8",
    "react": "15.4.1",
    "react-dom": "15.4.1",
    "react-router": "3.0.0",
    "whatwg-fetch": "2.0.1"
  }
}
